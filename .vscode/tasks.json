{
  "version": "2.0.0",
  "tasks": [
      {
          "label": "build",
          "command": "cl.exe",
          "args": [
              "/EHsc",
              "/Zi",
              "/DEBUG",
              "/Fe:",
              "${workspaceFolder}\\build\\CarRentalSystem.exe",
              "${workspaceFolder}\\src\\main.cpp",
              "${workspaceFolder}\\src\\car.cpp",
              "${workspaceFolder}\\src\\customer.cpp",
              "${workspaceFolder}\\src\\rental.cpp",
              "${workspaceFolder}\\src\\rentalService.cpp",
              "/I",
              "${workspaceFolder}/include"
          ],
          "type": "shell",
          "group": {
              "kind": "build",
              "isDefault": true
          },
          "problemMatcher": ["$msCompile"]
      }
  ]
}
